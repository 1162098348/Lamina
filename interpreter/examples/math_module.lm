// 定义一些常量
var PI = 3.1415926;
var E = 2.7182818;

// 定义数学函数
func abs(x) {
    if (x < 0) {
        return -x;
    }
    return x;
}

func square(x) {
    return x * x;
}

func cube(x) {
    return x * x * x;
}

func factorial(n) {
    if (n <= 1) {
        return 1;
    }
    return n * factorial(n - 1);
}

